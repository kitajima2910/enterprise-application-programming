@model Pretest_EAP2WEBAPP.Models.News

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete News</h1>

<h4>News</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Delete" asp-antiforgery="true">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NewsId" class="control-label"></label>
                <input asp-for="NewsId" class="form-control" />
                <span asp-validation-for="NewsId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<br />
&nbsp;&nbsp;&nbsp;<label class="text-danger">@ViewBag.Msg</label>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
